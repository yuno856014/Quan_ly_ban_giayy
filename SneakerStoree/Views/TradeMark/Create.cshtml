@model SneakerStoree.Models.TradeMark.CreateTradeMark

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>CreateTradeMark</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TradeMarkName" class="control-label"></label>
                <input asp-for="TradeMarkName" class="form-control" />
                <span asp-validation-for="TradeMarkName" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
            <a class="btn btn-secondary" asp-controller="TradeMark" asp-action="Index">
                <i class="fas fa-backward">
                </i>
                Back
            </a>
        </form>
    </div>  
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
